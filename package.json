{
  "name": "homepage-starter-kit",
  "version": "1.0.0",
  "description": "A frontend kit from several perspectives",
  "repository": "git@github.com:apeixinho/homepage-starter-kit.git",
  "author": "apeixinho <a_peixinho@sapo.pt>",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=8.12.0 || >=10.13.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn run clean && webpack --progress --config webpack.config.dev.js",
    "build:prod": "yarn run clean && webpack --progress -p --env production --config webpack.config.prod.js",
    "start": "yarn run build && webpack-dev-server --config webpack.config.dev.js",
    "start:prod": "yarn run build:prod && webpack-dev-server -p --env production --config webpack.config.prod.js"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions"
  ],
  "devDependencies": {
    "webpack-dev-server": "^3.1.10"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "autoprefixer": "^9.3.1",
    "babel-loader": "^8.0.4",
    "brotli-webpack-plugin": "^1.0.0",
    "clean-webpack-plugin": "^1.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "css-loader": "^1.0.1",
    "ejs": "^2.6.1",
    "ejs-compiled-loader": "^1.1.0",
    "eslint": "^5.8.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "express": "^4.16.4",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^2.0.0",
    "font-awesome": "^4.7.0",
    "font-awesome-loader": "^1.0.2",
    "helmet": "^3.14.0",
    "html-webpack-plugin": "^3.2.0",
    "jimp": "^0.6.0",
    "jquery": "^3.3.1",
    "node-sass": "^4.10.0",
    "optimize-css-assets-webpack-plugin": "^5.0.1",
    "pm2": "^3.2.2",
    "postcss-loader": "^3.0.0",
    "responsive-loader": "^1.1.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^1.3.0",
    "url-loader": "^1.1.2",
    "webpack": "^4.24.0",
    "webpack-cli": "^3.1.2"
  }
}
