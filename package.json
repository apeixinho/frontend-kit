{
  "name": "frontend-kit",
  "version": "1.0.0",
  "description": "A frontend kit from several perspectives",
  "repository": "git@github.com:apeixinho/frontend-kit.git",
  "author": "apeixinho <a_peixinho@sapo.pt>",
  "license": "GPL-3.0",
  "engines": {
    "node": "^8.11.3 || ^10.5.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn run clean && webpack --progress --config webpack.config.dev.js",
    "build:prod": "yarn run clean && webpack --progress -p --env production --config webpack.config.prod.js",
    "start": "yarn run build && webpack-dev-server --config webpack.config.dev.js",
    "start:prod": "yarn run build:prod && webpack-dev-server -p --env production --config webpack.config.prod.js"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions"
  ],
  "devDependencies": {
    "webpack-dev-server": "^3.1.5"
  },
  "dependencies": {
    "autoprefixer": "^9.0.2",
    "babel-core": "^6.26.3",
    "babel-loader": "^7.1.5",
    "babel-polyfill": "^7.0.0-beta.3",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^1.0.0",
    "ejs": "^2.6.1",
    "ejs-compiled-loader": "^1.1.0",
    "eslint": "^5.2.0",
    "eslint-loader": "^2.1.0",
    "eslint-plugin-import": "^2.13.0",
    "express": "^4.16.3",
    "extract-text-webpack-plugin": "^4.0.0-beta.0",
    "file-loader": "^1.1.11",
    "font-awesome": "^4.7.0",
    "font-awesome-loader": "^1.0.2",
    "google-fonts-webpack-plugin": "^0.4.4",
    "html-webpack-plugin": "^4.0.0-alpha",
    "jimp": "^0.3.1",
    "jquery": "^3.3.1",
    "node-sass": "^4.9.2",
    "pm2": "^3.0.3",
    "postcss-loader": "^2.1.6",
    "responsive-loader": "^1.1.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^4.16.4",
    "webpack-cli": "^3.1.0"
  }
}
