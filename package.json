{
  "name": "frontend-kit",
  "version": "1.0.0",
  "description": "A frontend kit from several perspectives",
  "repository": "git@github.com:apeixinho/frontend-kit.git",
  "author": "apeixinho <a_peixinho@sapo.pt>",
  "license": "GPL-3.0",
  "engines": {
    "node": "^8.11.2"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn run clean && webpack --progress --config webpack.config.dev.js",
    "build:prod": "yarn run clean && webpack --progress -p --env production --config webpack.config.prod.js",
    "start": "yarn run build && webpack-dev-server --config webpack.config.dev.js",
    "start:prod": "yarn run build:prod && webpack-dev-server -p --env production --config webpack.config.prod.js"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions"
  ],
  "devDependencies": {
    "autoprefixer": "^8.2.0",
    "babel-core": "^6.26.0",
    "babel-loader": "^7.1.4",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "ejs": "^2.6.1",
    "ejs-compiled-loader": "^1.1.0",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.0.0",
    "eslint-plugin-import": "^2.11.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^1.1.11",
    "font-awesome": "^4.7.0",
    "font-awesome-loader": "^1.0.2",
    "google-fonts-webpack-plugin": "^0.4.4",
    "html-webpack-plugin": "^3.2.0",
    "node-sass": "^4.8.3",
    "postcss-loader": "^2.1.5",
    "responsive-nav": "^1.0.39",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.0.1",
    "style-loader": "^0.21.0",
    "url-loader": "^1.0.1",
    "webpack": "^3.12.0",
    "webpack-dev-server": "^2.11.2"
  },
  "dependencies": {
    "babel-polyfill": "^6.26.0"
  }
}
