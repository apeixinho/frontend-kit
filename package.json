{
  "name": "homepage-starter-kit",
  "version": "1.0.0",
  "description": "A frontend kit from several perspectives",
  "repository": "git@github.com:apeixinho/homepage-starter-kit.git",
  "author": "apeixinho <a_peixinho@sapo.pt>",
  "license": "GPL-3.0",
  "engines": {
    "node": ">=8.12.0 || >=10.13.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn run clean && webpack --progress --config webpack.config.dev.js",
    "build:prod": "yarn run clean && webpack --progress -p --env production --config webpack.config.prod.js",
    "start": "yarn run build && webpack-dev-server --config webpack.config.dev.js",
    "start:prod": "yarn run build:prod && webpack-dev-server -p --env production --config webpack.config.prod.js"
  },
  "browserslist": [
    "last 3 Chrome versions",
    "last 3 Firefox versions"
  ],
  "devDependencies": {
    "webpack-dev-server": "^3.1.14"
  },
  "dependencies": {
    "@babel/core": "^7.2.2",
    "@babel/polyfill": "^7.2.5",
    "@babel/preset-env": "^7.2.3",
    "animate.css": "^3.7.0",
    "autoprefixer": "^9.4.4",
    "babel-loader": "^8.0.5",
    "brotli-webpack-plugin": "^1.0.0",
    "compression-webpack-plugin": "^2.0.0",
    "copy-webpack-plugin": "^4.6.0",
    "css-loader": "^2.1.0",
    "devicon-2.2": "^2.2.0",
    "ejs": "^2.6.1",
    "ejs-compiled-loader": "^1.1.0",
    "eslint": "^5.12.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "express": "^4.16.4",
    "file-loader": "^3.0.1",
    "font-awesome": "^4.7.0",
    "font-awesome-loader": "^1.0.2",
    "helmet": "^3.15.0",
    "html-webpack-plugin": "^4.0.0-beta.5",
    "jimp": "^0.6.0",
    "mini-css-extract-plugin": "^0.5.0",
    "node-sass": "^4.11.0",
    "pm2": "^3.2.4",
    "postcss-loader": "^3.0.0",
    "responsive-loader": "^1.2.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "^1.1.2",
    "vanillajs-scrollspy": "^2.0.3",
    "webpack": "^4.28.3",
    "webpack-cli": "^3.2.0"
  }
}
