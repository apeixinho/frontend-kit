{
  "name": "homepage-starter-kit",
  "version": "1.0.0",
  "description": "A frontend kit from several perspectives",
  "repository": "git@github.com:apeixinho/homepage-starter-kit.git",
  "author": "apeixinho <a_peixinho@sapo.pt>",
  "license": "GPL-3.0",
  "engines": {
    "node": "^8.12.0 || ^10.13.0"
  },
  "scripts": {
    "clean": "rimraf dist",
    "build": "yarn run clean && webpack --progress --config webpack.config.dev.js",
    "build:prod": "yarn run clean && webpack --progress -p --env production --config webpack.config.prod.js",
    "start": "yarn run build && webpack-dev-server --config webpack.config.dev.js",
    "start:prod": "yarn run build:prod && webpack-dev-server -p --env production --config webpack.config.prod.js"
  },
  "browserslist": [
    "last 2 Chrome versions",
    "last 2 Firefox versions"
  ],
  "devDependencies": {
    "webpack-dev-server": "^2.11.2"
  },
  "dependencies": {
    "@babel/core": "^7.1.2",
    "@babel/polyfill": "^7.0.0",
    "@babel/preset-env": "^7.1.0",
    "autoprefixer": "^9.3.1",
    "babel-loader": "^8.0.4",
    "clean-webpack-plugin": "^0.1.19",
    "css-loader": "^0.28.11",
    "ejs": "^2.6.1",
    "ejs-compiled-loader": "^1.1.0",
    "eslint": "^5.8.0",
    "eslint-loader": "^2.1.1",
    "eslint-plugin-import": "^2.14.0",
    "extract-text-webpack-plugin": "^3.0.2",
    "file-loader": "^2.0.0",
    "font-awesome": "^4.7.0",
    "font-awesome-loader": "^1.0.2",
    "google-fonts-webpack-plugin": "^0.4.4",
    "html-webpack-plugin": "^3.2.0",
    "jquery": "^3.3.1",
    "node-sass": "^4.10.0",
    "postcss-loader": "^3.0.0",
    "rimraf": "^2.6.2",
    "sass-loader": "^7.1.0",
    "style-loader": "^0.23.1",
    "url-loader": "~1.1.2",
    "webpack": "^3.12.0"
  }
}